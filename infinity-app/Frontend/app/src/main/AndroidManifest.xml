<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com">

<uses-permission android:name="android.permission.INTERNET" />

<application
    android:name=".app.AppController"
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:usesCleartextTraffic="true"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.Infinity">

    <activity android:name=".profile.ProfileActivity" />
    <activity android:name=".guild.GuildChatListActivity"
        android:theme="@style/Theme.Infinity.ChatList"/>
    <activity android:name=".dashboard.PrivateListActivity" />
    <activity android:name=".profile.NewPassword"
        android:theme="@style/Theme.Infinity.ChangePasswordPop"/>
    <activity android:name=".guild.GuildActivity"/>
    <activity android:name=".dashboard.DashActivity"
        android:exported="true"/>
    <activity android:name=".guild.GuildDrawingsActivity"/>
    <activity android:name=".guild.GuildNotesActivity"/>
    <activity
        android:name=".loginscreen.LoginActivity"
        android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name=".loginscreen.UserRegisterActivity"
        android:exported="false" />
    <activity
        android:name=".friends.FriendsList"
        android:exported="false" />
    <activity
        android:name=".friends.PendingFriendList"
        android:exported="false" />
    <activity
        android:name="com.friends.FindFriends"
        android:exported="false" />
    <activity
        android:name=".notes.NotesList"
        android:exported="false" />
    <activity
        android:name="com.notes.NotesEditor"
        android:exported="false" />

</application>

</manifest>